{
    "@context": "http://dev.openteamspace.com/vocab/context.jsonld",
    "@graph": [
        {
            "@type": "sh:NodeShape",
            "sh:targetClass": "ots:Collection",
            "sh:property": [
                {
                    "sh:path": "ots:name",
                    "ots:immutable": true,
                    "sh:labelTemplate": "List: "
                },
                {
                    "sh:path": "ots:collection-type",
                    "sh:nodeKind": "sh:IRI",
                    "sh:minCount": 1
                },
                {
                    "sh:path": "ots:items",
                    "sh:nodeKind": "sh:BlankNode",
                    "sh:minCount": 0,
                    "sh:labelTemplate": "Items"
                }
            ]
        },
        {
            "@type": "sh:NodeShape",
            "sh:targetClass": "ots:Team",
            "sh:property": [
                {
                    "sh:path": "@id",
                    "sh:nodeKind": "sh:IRI",
                    "sh:labelTemplate": "{ots:name @value}",
                    "sh:minCount": 1,
                    "sh:maxCount": 1
                },
                {
                    "sh:path": "ots:name",
                    "sh:dataType": "xsd:string",
                    "sh:pattern": ".{4,50}",
                    "sh:minCount": 1,
                    "sh:maxCount": 1,
                    "sh:labelTemplate": "Name",
                    "sh:description": "The name of your team"
                },
                {
                    "sh:path": "ots:scoresheet",
                    "sh:class": "ots:Scoresheet",
                    "sh:minCount": 0,
                    "sh:maxCount": 1,
                    "sh:labelTemplate": "Current scoresheet",
                    "chooseFrom": "ots:scoresheets",
                    "valueTemplate": "{ots:name @value}"
                },
                {
                    "sh:path": "ots:members",
                    "sh:nodeKind": "sh:IRI",
                    "sh:minCount": 0,
                    "sh:maxCount": 1,
                    "sh:labelTemplate": "Team members",
                    "immutable": true
                },
                {
                    "sh:path": "ots:scoresheets",
                    "sh:nodeKind": "sh:IRI",
                    "sh:minCount": 0,
                    "sh:maxCount": 1,
                    "sh:labelTemplate": "Scoresheets",
                    "immutable": true
                },
                {
                    "sh:path": "ots:scores",
                    "sh:nodeKind": "sh:IRI",
                    "sh:minCount": 0,
                    "sh:maxCount": 1,
                    "sh:labelTemplate": "Scores",
                    "immutable": true
                },
                {
                    "sh:path": "ots:goals",
                    "sh:nodeKind": "sh:IRI",
                    "sh:minCount": 0,
                    "sh:maxCount": 1,
                    "sh:labelTemplate": "Goals",
                    "immutable": true
                },
                {
                    "sh:path": "ots:periods",
                    "sh:nodeKind": "sh:IRI",
                    "sh:minCount": 0,
                    "sh:maxCount": 1,
                    "sh:labelTemplate": "Scoring periods",
                    "immutable": true
                }
            ]
        },
        {
            "@type": "sh:NodeShape",
            "sh:targetClass": "ots:Member",
            "sh:property": [
                {
                    "sh:path": "ots:name",
                    "sh:dataType": "xsd:string",
                    "sh:pattern": ".{2,50}",
                    "sh:minCount": 1,
                    "sh:maxCount": 1,
                    "sh:labelTemplate": "Name"
                }
            ]
        },
        {
            "@type": "sh:NodeShape",
            "sh:targetClass": "ots:Scoresheet",
            "sh:property": [
                {
                    "sh:path": "@id",
                    "sh:maxCount": 1,
                    "sh:nodeKind": "sh:IRI",
                    "sh:labelTemplate": "{ots:name @value}"
                },
                {
                    "sh:path": "ots:name",
                    "sh:minCount": 1,
                    "sh:maxCount": 1,
                    "ots:hidden": true,
                    "ots:summary": true,
                    "sh:labelTemplate": "Name"
                },
                {
                    "sh:path": "ots:created",
                    "sh:dataType": "xsd:dateTime",
                    "sh:minCount": 1,
                    "sh:maxCount": 1,
                    "ots:immutable": true
                },
                {
                    "sh:path": "ots:points",
                    "sh:dataType": "ots:Scoresheet.Point",
                    "sh:nodeKind": "sh:BlankNode",
                    "sh:labelTemplate": "Score menu"
                }
            ]
        },
        {
            "@type": "sh:NodeShape",
            "sh:targetClass": "ots:Scoresheet.Point",
            "sh:property": [
                {
                    "sh:path": "@id",
                    "sh:maxCount": 1,
                    "sh:nodeKind": "sh:IRI",
                    "sh:labelTemplate": "{ots:name @value}"
                },
                {
                    "sh:path": "ots:range",
                    "sh:dataType": "xsd:integer",
                    "sh:minCount": 2,
                    "sh:maxCount": 2
                },
                {
                    "sh:path": "ots:goal",
                    "sh:nodeKind": "sh:IRI",
                    "sh:class": "ots:Goal",
                    "sh:minCount": 1,
                    "sh:maxCount": 1,
                    "sh:labelTemplate": "Goal (name tbd)",
                    "sh:description": "The goal we are trying to achieve",
                    "chooseFrom": "ots:goals"
                },
                {
                    "sh:path": "ots:measure",
                    "sh:dataType": "xsd:string",
                    "sh:labelTemplate": "Measurement",
                    "sh:description": "What will be measured to determine points",
                    "sh:maxCount": 1
                }
            ]
        },
        {
            "@type": "sh:NodeShape",
            "sh:targetClass": "ots:Score",
            "sh:property": [
                {
                    "sh:path": "ots:created",
                    "sh:dataType": "xsd:dateTime",
                    "sh:minCount": 1,
                    "sh:maxCount": 1,
                    "sh:labelTemplate": "When"
                },
                {
                    "sh:path": "ots:member",
                    "sh:dataType": "ots:Member",
                    "sh:nodeKind": "sh:IRI",
                    "sh:minCount": 1,
                    "sh:maxCount": 1,
                    "sh:labelTemplate": "Who"
                },
                {
                    "sh:path": "ots:scoresheet",
                    "sh:dataType": "ots:Scoresheet",
                    "sh:nodeKind": "sh:IRI",
                    "sh:minCount": 1,
                    "sh:maxCount": 1,
                    "sh:labelTemplate": "From"
                },
                {
                    "sh:path": "ots:goal",
                    "sh:dataType": "ots:Goal",
                    "sh:nodeKind": "sh:IRI",
                    "sh:minCount": 1,
                    "sh:maxCount": 1,
                    "sh:labelTemplate": "What"
                },
                {
                    "sh:path": "ots:amount",
                    "sh:dataType": "xsd:integer",
                    "sh:minCount": 1,
                    "sh:maxCount": 1,
                    "sh:labelTempalte": "Score"
                },
                {
                    "sh:path": "ots:description",
                    "sh:dataType": "xsd:string",
                    "sh:minCount": 1,
                    "sh:maxCount": 1,
                    "sh:pattern": ".{5,500}",
                    "sh:labelTemplate": "Why"
                }
            ]
        },
        {
            "@type": "sh:NodeShape",
            "sh:targetClass": "ots:Goal",
            "sh:property": [
                {
                    "sh:path": "@id",
                    "sh:maxCount": 1,
                    "sh:nodeKind": "sh:IRI",
                    "sh:labelTemplate": "{ots:name @value}"
                },
                {
                    "sh:path": "ots:name",
                    "sh:dataType": "xsd:string",
                    "sh:pattern": ".{2,50}",
                    "sh:minCount": 1,
                    "sh:maxCount": 1
                }
            ]
        },
        {
            "@type": "sh:NodeShape",
            "sh:targetClass": "ots:Period",
            "sh:property": [
                {
                    "sh:path": "@id",
                    "sh:maxCount": 1,
                    "sh:nodeKind": "sh:IRI",
                    "sh:labelTemplate": "{ots:name @value}"
                },
                {
                    "sh:path": "ots:name",
                    "sh:dataType": "xsd:string",
                    "sh:pattern": ".{2,50}",
                    "sh:minCount": 1,
                    "sh:maxCount": 1
                }
            ]
        }
    ]
}